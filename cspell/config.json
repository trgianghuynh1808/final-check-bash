{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "dictionaryDefinitions": [
    {
      "name": "nodejs_words",
      "path": "./dictionaries/nodejs-words.txt",
      "addWords": true
    },
    {
      "name": "react-native_words",
      "path": "./dictionaries/react-native_words.txt",
      "addWords": true
    }
  ],
  "dictionaries": ["nodejs_words", "react-native_words"],
  "ignorePaths": [
    "./dictionaries/**",
    "../temp/android/**",
    "../temp/ios/**",
    "../temp/yarn.lock",
    "../temp/**/**.json",
    "../temp/**/constants/**",
    "../temp/**/constants.**"
  ]
}
